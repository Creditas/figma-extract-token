{
  "source": ["build/**/*.json"],
  "platforms": {
    "web": {
      "transformGroup": "js",
      "buildPath": "build/web/",
      "files": [{
        "destination": "index.ts",
        "format": "javascript/es6"
      }]
    },
    "mobile": {
      "transformGroup": "reactNative",
      "buildPath": "build/mobile/",
      "files": [{
        "destination": "index.ts",
        "format": "javascript/es6"
      }]
    }
  }
}
